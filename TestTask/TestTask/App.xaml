             xmlns:local="clr-namespace:TestTask">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Buttons/Buttons.xaml" />
                <ResourceDictionary Source="Resources/Frames/Frames.xaml" />
                <ResourceDictionary Source="Resources/Text/TextBoxes.xaml" />
                <ResourceDictionary Source="Resources/Text/PasswordBoxes.xaml" />
